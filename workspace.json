{
  "version": 2,
  "projects": {
    "jspenc-web": {
      "root": "apps/jspenc-web",
      "sourceRoot": "apps/jspenc-web",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/jspenc-web",
            "outputPath": "dist/apps/jspenc-web"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "jspenc-web:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "jspenc-web:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "jspenc-web:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/jspenc-web"],
          "options": {
            "jestConfig": "apps/jspenc-web/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/jspenc-web/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "jspenc-web-e2e": {
      "root": "apps/jspenc-web-e2e",
      "sourceRoot": "apps/jspenc-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jspenc-web-e2e/cypress.json",
            "tsConfig": "apps/jspenc-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "jspenc-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jspenc-web:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/jspenc-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-shared-ui": {
      "root": "libs/ui/shared-ui",
      "sourceRoot": "libs/ui/shared-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/shared-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/shared-ui"],
          "options": {
            "jestConfig": "libs/ui/shared-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/shared-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-shared-ui",
            "config": {
              "configFolder": "libs/ui/shared-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-shared-ui-e2e": {
      "root": "apps/ui-shared-ui-e2e",
      "sourceRoot": "apps/ui-shared-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-shared-ui-e2e/cypress.json",
            "tsConfig": "apps/ui-shared-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-shared-ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-shared-ui:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ui-shared-ui-e2e/**/*.{js,ts}"]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/ui-shared-ui-e2e/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-shared-ui-e2e",
            "config": {
              "configFolder": "apps/ui-shared-ui-e2e/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "jspenc-web"
}
